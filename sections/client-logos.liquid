<div class="client-logo-section">
  <!--container as module-->
  <div class="module">
    <div class="section-title">
    <h2>{{ section.settings.text-box}}</h2>
      </div>
    <div class="row justify-content-center">
       {% for block in section.blocks %}
      <div class="col-md-2 col-sm-6 inner-client-logo">
        <img class="client Logo" src="{{ block.settings.client-logo | img_url: "" }}" alt="">
      </div>
      {% endfor %}
    </div>
  </div>
</div>


{% schema %}
  {
    "name": "Client Logo Section",
    "settings": [
	{
      "id": "text-box",
      "type": "text",
      "label": "Heading",
      "default": "Title"
    }
	],
    "blocks": [
      {
        "type": "select",
        "name": "Add Client Logo",
        "settings": [
          {
            "id": "client-logo",
            "type": "image_picker",
            "label": "Client Logo"
          }
        ]
      }
    ],
  "presets": [
        {
         "name": "Client Logo Section",
			"blocks": [
            { 
              "type": "select"
            }
		  ]
        }
      ]
  }
{% endschema %}

